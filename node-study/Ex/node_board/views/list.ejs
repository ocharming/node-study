<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<title><%=title%></title>
	<link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
	<h1><%=title%></h1>
	<a href="/write">글 쓰기</a>
	<a href="/">처음화면</a>
	<table width="560" border="0">
		<tr>
			<th>번호</th>
			<th>제목</th>
			<th>작성자</th>
			<th>날짜</th>
			<th>조회</th>
		</tr>
		<% for(var i = 0; i < data.length; i++) { %>
		<tr>
			<td><%=data[i].num%></td>
			<td><a href="/read/<%=data[i].num%>"><%=data[i].title%></a></td>
			<td><%=data[i].writer%></td>
			<td><%=data[i].regdate%></td>
			<td><%=data[i].hit%></td>
		</tr>
	        <% } %> 
	</table>
</body>
</html>